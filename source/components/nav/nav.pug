mixin nav()
  .nav
    a(href="#smiles").nav__item
      +icon('tooth-sparkle')
      .nav__label: span Every Smile. Every Day.
    a(href="#initiative").nav__item
      +icon('ribbon')
      .nav__label: span Grant-making Highlights
    a(href="#partner").nav__item
      +icon('handshake')
      .nav__label: span Initiatives
    a(href="#finances").nav__item
      +icon('piggy')
      .nav__label: span Finances & Who We Are