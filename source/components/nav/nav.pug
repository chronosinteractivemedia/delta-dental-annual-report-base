mixin nav()
  .nav
    a(href="#smiles").nav__item
      +icon('tooth-sparkle')
      .nav__label: span We're All Smiles
    a(href="#initiative").nav__item
      +icon('ribbon')
      .nav__label: span Initiative Highlights
    a(href="#partner").nav__item
      +icon('handshake')
      .nav__label: span Partner Highlights
    a(href="#finances").nav__item
      +icon('piggy')
      .nav__label: span Finances & Who We Are